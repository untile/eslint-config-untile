{
  "name": "@untile/eslint-config-untile",
  "version": "0.0.5",
  "description": "Untile-flavored ESLint config",
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "homepage": "https://github.com/untile/eslint-config-untile#readme",
  "bugs": {
    "url": "https://github.com/untile/eslint-config-untile/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/untile/eslint-config-untile.git"
  },
  "license": "MIT",
  "author": "Untile",
  "main": "src",
  "scripts": {
    "changelog": "echo \"$(github-changelog-generator --future-release=v$npm_package_version)\n$(tail -n +2 CHANGELOG.md)\" > CHANGELOG.md",
    "lint": "eslint src test/index.js",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "test": "mocha $npm_package_options_mocha",
    "version": "npm run changelog && git add -A CHANGELOG.md"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "eslint-plugin-jest": "26.1.5",
    "eslint-plugin-mocha": "10.0.4",
    "eslint-plugin-new-with-error": "3.1.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-imports-es6": "^0.0.3",
    "eslint-plugin-switch-case": "^1.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@untile/commitlint-config-untile": "^1.0.0",
    "@uphold/github-changelog-generator": "^3.0.0",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "mocha": "^9.2.2",
    "should": "13.2.3",
    "sort-package-json": "^1.55.0"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "engines": {
    "node": "14.7.0 || >=16.0.0"
  },
  "options": {
    "mocha": "-t 10000 --require should test"
  },
  "validate-branch-name": {
    "pattern": "^(master|development){1}$|^(bugfix|dependabot|enhancement|feature|hotfix|release|support)/.+$",
    "errorMsg": "Only the following prefixes are allowed: bugfix|dependabot|enhancement|feature|hotfix|release|support"
  }
}
