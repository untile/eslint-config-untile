{
  "name": "@untile/eslint-config-untile",
  "version": "0.0.4",
  "description": "Untile-flavored ESLint config",
  "keywords": [
    "config",
    "eslint",
    "lint"
  ],
  "homepage": "https://github.com/untile/eslint-config-untile#readme",
  "bugs": {
    "url": "https://github.com/untile/eslint-config-untile/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/untile/eslint-config-untile.git"
  },
  "license": "MIT",
  "author": "Untile",
  "main": "src",
  "scripts": {
    "changelog": "github-changelog-generator --future-release=v$npm_package_version > CHANGELOG.md",
    "lint": "eslint src test/index.js",
    "lint-staged": "lint-staged",
    "test": "mocha $npm_package_options_mocha",
    "version": "npm run changelog && git add -A CHANGELOG.md"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "src/**/*.js": "eslint",
    "package.json": "sort-package-json"
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-mocha": "5.3.0",
    "eslint-plugin-new-with-error": "2.0.0",
    "eslint-plugin-sort-destructure-keys": "1.3.5",
    "eslint-plugin-sort-imports-es6": "^0.0.3",
    "eslint-plugin-sql-template": "^2.0.0",
    "eslint-plugin-switch-case": "^1.1.2"
  },
  "devDependencies": {
    "@uphold/github-changelog-generator": "^1.0.1",
    "eslint": "^7.32.0",
    "lint-staged": "^10.5.4",
    "mocha": "^7.1.1",
    "pre-commit": "^1.2.2",
    "should": "13.2.3",
    "sort-package-json": "^1.52.0"
  },
  "peerDependencies": {
    "eslint": "^7.32.0"
  },
  "engines": {
    "node": ">=10.23.2"
  },
  "options": {
    "mocha": "-t 10000 --require should test"
  }
}
